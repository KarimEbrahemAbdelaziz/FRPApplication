# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'FRPApplication' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FRPApplication
  pod 'RxSwift'
  pod 'RxCocoa'

  target 'FRPApplicationTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RxBlocking'
    pod 'RxTest'

  end

  target 'FRPApplicationUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
